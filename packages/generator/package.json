{
  "name": "@flethy/generator",
  "version": "1.0.0",
  "description": "toolkit for web services",
  "keywords": [],
  "main": "index.ts",
  "scripts": {
    "start": "NODE_ENV=local ts-node ./src/index.ts",
    "marketing": "NODE_ENV=local ts-node ./src/controllers/NavigateToWebsites.ts",
    "export": "npm run exportexec && npm run reflectgen && npm run updateapicount && npm run seo",
    "exportexec": "NODE_ENV=local ts-node ./src/export.ts",
    "reflectgen": "NODE_ENV=local ts-node -P tsconfig-tt.json ./src/exporters/reflect.generator.ts",
    "updateapicount": "mustache data.json ./src/docs/connectors.README.md > ../connectors/README.md",
    "seo:landingpage": "mustache ./src/seo/seodata.json ./src/seo/landingpage.index.html > ../../apps/landing-page/index.html",
    "seo": "npm run seo:landingpage",
    "config:create": "ts-node ./src/utils/Template.utils.ts"
  },
  "author": "adam@flethy.com",
  "license": "ISC",
  "dependencies": {
    "@flethy/connectors": "workspace:*",
    "dotenv": "^16.0.3",
    "markdown-json": "^1.6.1",
    "node-fetch": "^3.3.0",
    "pino": "^8.8.0",
    "puppeteer": "^19.6.3",
    "quicktype-core": "^21.0.9",
    "tst-reflect": "^0.12.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/jasmine": "^4.3.1",
    "@types/jest": "^29.4.0",
    "@types/node": "18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "eslint": "^8.33.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.4.1",
    "mustache": "^4.2.0",
    "pino-pretty": "^9.1.1",
    "prettier": "2.8.3",
    "ts-node": "^10.9.1",
    "tsconfig": "workspace:*",
    "tst-reflect-transformer": "^0.12.5",
    "ttypescript": "^1.5.15",
    "typescript": "^4.9.5"
  }
}
